<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.brickdestroygame.DebugConsole">
   <children>
      <VBox prefHeight="400.0" prefWidth="150.0">
         <children>
            <Button mnemonicParsing="false" onAction="#skipLevelButton" prefHeight="34.0" prefWidth="150.0" text="Skip Levels" />
            <Slider fx:id="levelSlider" blockIncrement="1.0" majorTickUnit="1.0" max="6.0" min="1.0" minorTickCount="0" onDragDetected="#skipLevelSlider" showTickLabels="true" showTickMarks="true" snapToTicks="true" />
         </children>
      </VBox>
      <VBox prefHeight="400.0" prefWidth="150.0">
         <children>
            <Button mnemonicParsing="false" onAction="#resetBallCountButton" prefHeight="34.0" prefWidth="150.0" text="Reset Ball Count" />
            <Slider fx:id="ballCountSlider" blockIncrement="1.0" majorTickUnit="1.0" max="3.0" min="1.0" minorTickCount="0" onDragDetected="#ballSlider" showTickLabels="true" showTickMarks="true" snapToTicks="true" />
         </children>
      </VBox>
   </children>
</HBox>
